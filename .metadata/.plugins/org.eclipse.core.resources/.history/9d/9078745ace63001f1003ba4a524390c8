package com.JavaBasedConfig;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class JavaConfig {
@Bean
Rhino rhino() {
//	return new Rhino();
	
	Rhino rhino= new Rhino();
	rhino.setWeight(150);
	return rhino;
}
@Bean 
Danfe danfe() {
	Danfe d= new Danfe();
	d.setColor("Blue");
	d.setWeight(10);
	return d;
}
}
